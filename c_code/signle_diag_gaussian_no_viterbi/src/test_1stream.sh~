#!/bin/bash

MATLAB="/usr/local/MATLAB/R2011a/bin/matlab -nodesktop -nojvm -nosplash -nodisplay"
CFG_FILE=../config/ib.cfg 
source $CFG_FILE

# ES2002a ES2006a ES2007a ES2007d ES2009b ES2002b ES2002d ES2003b ES2003c ES2004b ES2005b ES2005d  ES2006b ES2006c ES2006d EN2001b EN2002a EN2004a EN2002b IS1008a
for id in TS3003b TS3003c TS3003d TS3004a TS3004d TS3005b TS3005c TS3006a TS3006b TS3006c TS3006d TS3007a TS3007c TS3007d TS3008a TS3008b TS3008c TS3008d TS3009b TS3009c TS3009d  TS3010b TS3010c TS3011b TS3011d TS3012b
#ES2008d  ES2009c ES2009d ES2011d  ES2012b ES2012c ES2012d ES2013c ES2014a ES2014b ES2014c ES2015b ES2015c ES2015d ES2016a ES2016c ES2016d IB4001 IB4002 IB4004 IB4005 IB4011 IN1001 IN1002 IN1005 IN1008
#IS2007a EN2002d EN2003a EN2005a EN2006a EN2006b EN2009b EN2009c EN2009d     ES2008c  TS3010a ES2004a ES2008a ES2010d ES2011a ES2012a 
# IN1009 IN1012 IN1013 IN1014 IS1000a IS1000c IS1000d IS1001a IS1001b IS1001c IS1001d IS1002c IS1002d IS1003a IS1003c IS1004a IS1004c IS1004d IS1005a IS1005b IS1006a IS1006b IS1006c IS1006d  IS1007b IS1007c  IS1008b IS1009a IS1009d temp 
do
     #source /etc/environment
     
    echo "$id";     
    ./pre ~/btp/data/diarization_data/mfcc/$id.fea ~/btp/data/diarization_data/gt.scp/$id.scp
    
done

